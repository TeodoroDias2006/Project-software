generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  /// Multiple data types found: String: 75%, BigInt: 25% out of 4 sampled entries
  age      String
  email    String  @unique
  name     String
  password String?
}
